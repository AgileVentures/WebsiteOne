<div class="row">
  <h4 id="video_contents"><%= presenter.title %></h4>

  <div class="col-xs-12 text-center">
    <div class="video-container">
      <iframe id="ytplayer" type="text/html" src="<%= presenter.video_embed_link %>" frameborder="0"></iframe>
    </div>
  </div>
</div>
<div class="row" id="video_links">
  <table class="table table-bordered table-striped table-hover table-condensed">
    <tr>
      <th>Title</th>
    </tr>
    <% videos.each do |video| %>
      <% present video do |presenter| %>
        <tr>
          <td><%= presenter.video_link %><small> <%= video.created_at.strftime '%A, %B %-d, %Y'  %></small></td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>
