<%= content_for :content do %>
  <div id="sidebar-container"class="container">
    <nav id="sidebar" class="col-sm-3" role="complementary">
    <%= render partial: 'layouts/sidebar', locals: { project: @project, document: @document } %>
    </nav>
    <div id="not-sidebar" class="col-sm-9 pull-right">
      <%= yield %>
    </div>
  </div>
<% end %>
<%= render template: 'layouts/application' %>